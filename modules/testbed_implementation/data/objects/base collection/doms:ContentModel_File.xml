<?xml version="1.0" encoding="UTF-8"?>

<foxml:digitalObject
        VERSION="1.1"
        PID="doms:ContentModel_File"
        xmlns:foxml="info:fedora/fedora-system:def/foxml#"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation=
                "info:fedora/fedora-system:def/foxml#
                http://www.fedora.info/definitions/1/0/foxml1-1.xsd">


    <foxml:objectProperties>
        <foxml:property
                NAME="info:fedora/fedora-system:def/model#state"
                VALUE="A"/>
        <foxml:property
                NAME="info:fedora/fedora-system:def/model#label"
                VALUE="Content Model for File Objects"/>
    </foxml:objectProperties>

    <foxml:datastream ID="DC" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
        <foxml:datastreamVersion ID="DC.0" MIMETYPE="text/xml" LABEL="Default Dublin Core Record"
                                 FORMAT_URI="http://www.openarchives.org/OAI/2.0/oai_dc/">

            <foxml:xmlContent>
                <oai_dc:dc
                        xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/"
                        xmlns:dc="http://purl.org/dc/elements/1.1/">
                    <dc:title>File</dc:title>
                    <dc:description>Content Model File</dc:description>
                    <dc:identifier>doms:ContentModel_File</dc:identifier>
                </oai_dc:dc>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="RELS-EXT" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion  ID="RELS-EXT.1" LABEL="Relationships"
                                  FORMAT_URI="info:fedora/fedora-system:FedoraRELSExt-1.0"
                                  MIMETYPE="application/rdf+xml" >
            <foxml:contentDigest DIGEST="none" TYPE="DISABLED"/>
            <foxml:xmlContent>
                <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                         xmlns:doms="http://doms.statsbiblioteket.dk/relations/default/0/1/#"
                         xmlns:ecm="http://ecm.sourceforge.net/relations/0/2/#"
                         xmlns:fedora="info:fedora/fedora-system:def/model#">
                    <rdf:Description rdf:about="info:fedora/doms:ContentModel_File">
                        <fedora:hasModel rdf:resource="info:fedora/fedora-system:ContentModel-3.0"/>
                        <fedora:hasModel rdf:resource="info:fedora/doms:ContentModel_DOMS"/>
                        <ecm:extendsModel rdf:resource="info:fedora/doms:ContentModel_DOMS" />
                        <doms:isPartOfCollection rdf:resource="info:fedora/doms:DOMS_Base_Collection"/>
                        <doms:hasLicense rdf:resource="info:fedora/doms:Open_License" />
                    </rdf:Description>
                </rdf:RDF>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="VIEW" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion ID="VIEW1.0" LABEL="VIEW datastream" MIMETYPE="text/xml">
            <foxml:xmlContent>
                <v:views xmlns:v="http://ecm.sourceforge.net/types/view/0/2/#">
                </v:views>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="DS-COMPOSITE-MODEL" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion  ID="DS-COMPOSITE-MODEL.0" LABEL="DS Composite Model"
                                  MIMETYPE="text/xml"
                                  FORMAT_URI="info:fedora/fedora-system:FedoraDSCompositeModel-1.0">
            <foxml:xmlContent>
                <dsCompositeModel
                        xmlns="info:fedora/fedora-system:def/dsCompositeModel#"
                        xmlns:schema="http://doms.statsbiblioteket.dk/types/dscompositeschema/0/1/#">
                    <dsTypeModel ID="CHARACTERISATION">
                        <form MIME="text/xml"/>
                        <extension name="SCHEMA">
                            <reference type="datastream" value="CHARACTERISATION_SCHEMA"/>
                        </extension>
                    </dsTypeModel>
                    <dsTypeModel ID="CONTENTS">
                        <form MIME="application/octet-stream"/>
                    </dsTypeModel>
                </dsCompositeModel>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="ONTOLOGY" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion
                ID="ONTOLOGY.0"
                LABEL="Local ontology"
                FORMAT_URI="info:fedora/fedora-system:FedoraOntology-1.0"
                MIMETYPE="application/rdf+xml">
            <foxml:xmlContent>

                <rdf:RDF
                        xmlns:owl="http://www.w3.org/2002/07/owl#"
                        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
                    <owl:Class rdf:about="info:fedora/doms:ContentModel_File#class">
                        <rdfs:subClassOf>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://doms.statsbiblioteket.dk/relations/default/0/1/#hasOriginal"/>
                                <owl:allValuesFrom rdf:resource="info:fedora/doms:ContentModel_File#class"/>
                            </owl:Restriction>
                        </rdfs:subClassOf>
                    </owl:Class>

                    <owl:ObjectProperty rdf:about="http://doms.statsbiblioteket.dk/relations/default/0/1/#hasOriginal"/>
                </rdf:RDF>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="CHARACTERISATION_SCHEMA" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion
                ID="CHARACTERISATION_SCHEMA.0"
                MIMETYPE="text/xml"
                LABEL="CHARACTERISATION schema">
            <foxml:xmlContent>
                <xs:schema xmlns:tns="http://doms.statsbiblioteket.dk/types/characterisation/0/2/#"
                           xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0"
                           targetNamespace="http://doms.statsbiblioteket.dk/types/characterisation/0/2/#">

                    <xs:element name="Characterisation" type="tns:characterisation"/>

                    <xs:complexType name="characterisation">
                        <xs:sequence>
                            <xs:element name="formatURI" type="xs:anyURI"
                                        maxOccurs="unbounded"/>
                            <xs:element name="bestFormat" type="xs:anyURI"/>
                            <xs:element name="validationStatus" type="xs:string"/>
                            <xs:element name="identifyServiceDescriptions" type="xs:string"
                                        minOccurs="0" maxOccurs="unbounded"/>
                            <xs:element name="identifyServiceReports" type="xs:string"
                                        minOccurs="0" maxOccurs="unbounded"/>
                            <xs:element name="validateServiceDescriptions" type="xs:string"
                                        minOccurs="0" maxOccurs="unbounded"/>
                            <xs:element name="validateServiceReports" type="xs:string"
                                        minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:schema>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream CONTROL_GROUP="X" ID="PLANETS" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion ID="PLANETS1.0" LABEL="Planets decode" MIMETYPE="text/xml">
            <foxml:xmlContent>
                <planetsDatastream>
                    <contentdatastream>
                        <name>CONTENTS</name>
                    </contentdatastream>
                    <metadatastreams>
                        <metadatastream>
                            <planetsName>DC</planetsName>
                            <name>DC</name>
                        </metadatastream>
                    </metadatastreams>
                </planetsDatastream>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>

    <foxml:datastream
            CONTROL_GROUP="E"
            ID="POLICY"
            STATE="A"
            VERSIONABLE="true">
        <foxml:datastreamVersion
                ID="POLICY.0"
                LABEL="License for this object"
                MIMETYPE="text/xml">
            <foxml:contentDigest DIGEST="none" TYPE="DISABLED"/>
            <foxml:contentLocation REF="http://local.fedora.server/fedora/objects/doms:Open_License/datastreams/LICENSE/content" TYPE="URL"/>
        </foxml:datastreamVersion>
    </foxml:datastream>

</foxml:digitalObject>
